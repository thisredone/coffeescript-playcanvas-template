{
    "name": "coffeescript-playcanvas-template",
    "version": "1.0.1",
    "description": "A template for running ES6 with PlayCanvas Editor",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack --config webpack.config.js && cross-env UPLOAD=no NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8081 --inline --hot",
        "https": "cross-env NODE_ENV=development webpack --config webpack.config.js && cross-env UPLOAD=no NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8081 --inline --hot --https",
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.js"
    },
    "keywords": [
        "PlayCanvas"
    ],
    "author": "redka",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thisredone/coffeescript-playcanvas-template.git"
    },
    "bugs": {
        "url": "https://github.com/thisredone/coffeescript-playcanvas-template/issues"
    },
    "homepage": "https://github.com/thisredone/coffeescript-playcanvas-template#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "coffee-loader": "^0.9.0",
        "coffeescript": "^2.3.2",
        "cross-env": "^5.0.5",
        "playcanvas-webpack-plugin": "^1.0.3",
        "raw-loader": "^0.5.1",
        "webpack": "^3.5.6",
        "webpack-dev-server": "^2.7.1",
        "webpack-merge": "^4.2.1"
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0"
    }
}
